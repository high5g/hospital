<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="Ordonnance" table="ordonnance">
    <indexes>
      <index name="fk_id_consultation" columns="fk_id_consultation"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="IDENTITY"/>
    </id>
    <field name="notes" type="string" column="notes" length="500" nullable="false"/>
    <many-to-one field="fkConsultation" target-entity="Consultation">
      <join-columns>
        <join-column name="fk_id_consultation" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
