{% block table %}
    <div>
        List of patients in {{ hospitalName }}:
    </div>
    <table>
        <tr>
            <th>First name</th>
            <th>Last name</th>
        </tr>
        {% for patient in patients %}
            <tr>
                <td>{{ patient.prenom }}</td>
                <td>{{ patient.nom }}</td>
            </tr>
        {% endfor %}
    </table>
{% endblock %}
